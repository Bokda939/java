import java.util.Scanner;
class temp {
double max,min,maxf,minf;
temp() {
 }
temp(double m,double n) {
max=m;
min=n; }
void compute() {
maxf=9*max/5+32;
minf=9*min/5+32; }
void display() {
System.out.println("maxf="+maxf);
System.out.println("minf="+minf); }
public static void main(String args[]) {
double m,n;
Scanner sc = new Scanner(System.in);
System.out.println("Enter maximum and minimum temperature in celsius scale:");
m=sc.nextDouble();
n=sc.nextDouble();
temp t = new temp();
t.compute();
t.display();
temp t1 = new temp(m,n);
t1.compute();
t1.display(); } }
